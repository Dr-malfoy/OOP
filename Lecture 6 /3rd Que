using System;
namespace que_three
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int[] arr = new int[n];
            int sum = 0;
            for (int i = 0; i < n; i++)
            {
                arr[i] = Convert.ToInt32(Console.ReadLine());
            }
            for (int i = n-1; i >= 0; i--)
            {
                Console.WriteLine(arr[i]);
            }
        }
    }
}
